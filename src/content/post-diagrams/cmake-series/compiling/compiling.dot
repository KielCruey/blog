digraph compiling {
    subgraph g {
        label="C++ Compiling Process"
        labelloc = t; // Places the cluster label at the top
        style = filled;
        color = lightgrey;

        node [shape=box, style="filled", fillcolor=white]
        sc [label="Source Code"]
        p [label="Preprocessor"]
        c [label="Compiler"]
        a [label="Assembler"]
        l [label="Linker"]   
        e [label="Executable"]
        
        node [shape=ellipse, style="filled", fillcolor=white]
        lib [label="external\nlibrary"]

        node[shape=box3d, style="filled", fillcolor=white]
        ppe [label="stop if pre-processor error"]
        ce [label="stop if syntax error"]
        le [label="stop if linker error"]

        edge [arrowhead=normal, arrowtail=dot]
        sc -> p [label=" .c .h .cpp .hpp"];
        p -> c [label=" .i"];
        c -> a [label=" .s"];
        a -> l [label=" .o"];
        l -> e [label=" ./<file-name>\n.exe\n.lib\n.a\n.so\n.dll"];
        
        p -> ppe [style=dashed, color=red]
        c -> ce [style=dashed, color=red]
        l -> le [style=dashed, color=red]
        lib -> l [label="[optional]", style=dashed, color=green]
    }
}